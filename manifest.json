{
  "manifest_version": 3,
  "name": "Yen 2 USD",
  "description": "An extension to automatically convert asian prices including YEN, YUAN, and WON to USD within the same page",
  "version": "1.0",
  "action": {
    "default_popup": "hello.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://realestate.co.jp/*"
      ]
    }
  ],
  "permissions": [
    "storage"
  ]
}